# worker-service/.env

# Directory Settings
UPLOAD_DIR=storage/uploads
OUTPUT_DIR=storage/outputs
TEMP_DIR=storage/temp
FACEFUSION_SCRIPT=facefusion.py
SETTINGS_FILE=facefusion.ini

# Service Configuration
SERVICE_HOST=0.0.0.0
SERVICE_PORT=8000
LOG_LEVEL=INFO

# Processing Settings
MAX_FACES_FOR_CLASSIFICATION=12
EXECUTION_PROVIDERS=["cpu"]

# FaceSwap Configuration
FACESWAP_DEFAULT_PROCESSORS=["face_swapper"]
FACESWAP_DEFAULT_FACE_DETECTOR=yoloface
FACESWAP_DEFAULT_FACE_SELECTOR_MODE=many

# --- Redis --- 
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_DB=0
REDIS_URL="redis://${REDIS_HOST}:${REDIS_PORT}/${REDIS_DB}"
